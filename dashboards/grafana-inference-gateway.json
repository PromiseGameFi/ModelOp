{
  "title": "ModelOp Inference Gateway",
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "10s",
  "panels": [
    {
      "id": 1,
      "title": "Request Outcomes / s",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 },
      "targets": [
        {
          "expr": "sum by (reason) (rate(gateway_requests_total[1m]))",
          "legendFormat": "{{reason}}"
        }
      ]
    },
    {
      "id": 2,
      "title": "TTFT p95 (ms)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum by (le, tenant_id) (rate(request_ttft_seconds_bucket[5m]))) * 1000",
          "legendFormat": "{{tenant_id}}"
        }
      ]
    },
    {
      "id": 3,
      "title": "TPOT p95 (ms)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 8 },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum by (le, tenant_id) (rate(request_tpot_seconds_bucket[5m]))) * 1000",
          "legendFormat": "{{tenant_id}}"
        }
      ]
    },
    {
      "id": 4,
      "title": "KV Cache Utilization",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 6, "x": 12, "y": 8 },
      "targets": [
        {
          "expr": "kv_cache_utilization_ratio",
          "legendFormat": "utilization"
        }
      ]
    },
    {
      "id": 5,
      "title": "Queue Depth",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 6, "x": 18, "y": 8 },
      "targets": [
        {
          "expr": "queue_depth",
          "legendFormat": "queue"
        }
      ]
    }
  ]
}
